{
    "name": "ulue/phpgit",
    "type": "library",
    "description": "A Git wrapper for PHP 7.1+",
    "homepage": "https://github/ulue/phpgit.git",
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "inhere",
            "email": "in.798@qq.com"
        },
        {
            "name": "Kazuyuki Hayashi",
            "email": "hayashi@valnur.net"
        }
    ],
    "require": {
        "php": ">7.1.0",
        "toolkit/stdlib":"~1.0",
        "symfony/process":"~4.0",
        "symfony/options-resolver": "~4.0"
    },
    "require-dev": {
        "symfony/filesystem": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "PhpGit\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
